<!DOCTYPE node PUBLIC
"-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">

  <interface name="org.freedesktop.URfkill">
    <method name="Block">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="s" name="type" direction="in" />
      <arg type="b" name="block" direction="in" />
      <arg type="b" name="ret" direction="out" />
    </method>
    <method name="BlockIdx">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="u" name="index" direction="in" />
      <arg type="b" name="block" direction="in" />
      <arg type="b" name="ret" direction="out" />
    </method>
    <method name="EnumerateDevices">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="ao" name="array" direction="out" />
    </method>
    <method name="GetKillswitch">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="u" name="index" direction="in" />
      <arg type="i" name="type"  direction="out" />
      <arg type="i" name="state" direction="out" />
      <arg type="u" name="soft"  direction="out" />
      <arg type="u" name="hard"  direction="out" />
      <arg type="s" name="name"  direction="out" />
    </method>
    <method name="KeyControlEnabled">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="b" name="ret" direction="out" />
    </method>
    <method name="EnableKeyControl">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="b" name="enable" direction="in" />
      <arg type="b" name="ret" direction="out" />
    </method>

    <signal name="RfkillAdded">
      <arg type="u" name="index" direction="out" />
      <arg type="u" name="type"  direction="out" />
      <arg type="i" name="state" direction="out" />
      <arg type="u" name="soft"  direction="out" />
      <arg type="u" name="hard"  direction="out" />
      <arg type="s" name="name"  direction="out" />
    </signal>
    <signal name="RfkillRemoved">
      <arg type="u" name="index" direction="out" />
    </signal>
    <signal name="RfkillChanged">
      <arg type="u" name="index" direction="out" />
      <arg type="u" name="type"  direction="out" />
      <arg type="i" name="state" direction="out" />
      <arg type="u" name="soft"  direction="out" />
      <arg type="u" name="hard"  direction="out" />
      <arg type="s" name="name"  direction="out" />
    </signal>

    <property name="DaemonVersion" type="s" access="read">
    </property>

  </interface>
</node>
